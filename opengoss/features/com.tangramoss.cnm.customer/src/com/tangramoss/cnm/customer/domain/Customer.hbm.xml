<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tangramoss.cnm.customer.domain">
	
	<class name="Customer" table="customer" lazy="false">
        <id name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
		<property name="name" length="255"/>
		<property name="fullName" length="255"/>
		<property name="customerClass" length="255"/>
		<property name="linkMan" length="255"/>
		<property name="level" length="255" />
		<property name="phone" length="255"/>
		<property name="mobile" length="255"/>
		<property name="corperator" length="255"/>
		<property name="address" length="255"/>
		<property name="informed" type="java.lang.Boolean"/>
		<property name="message" column="messageNo" length="255"/>
		<property name="email" length="255"/>
		<property name="region" length="255"/>
		<property name="circuitNo" length="255"/>
		<property name="circuitInfo" length="255"/>
		<property name="fax" length="255"/>
		<property name="description"/>
		<property name="time"  type="long"/>
    </class>
	
</hibernate-mapping>
