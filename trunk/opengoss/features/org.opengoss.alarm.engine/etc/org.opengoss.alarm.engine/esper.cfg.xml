<?xml version="1.0" encoding="UTF-8"?>
<esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<event-type alias="RawEvent">
		<java-util-map>
			<map-property name="node_addr" class="string"/>
			<map-property name="time_stamp" class="long"/>
		</java-util-map>
	</event-type>
	<event-type alias="AlarmEvent" class="org.opengoss.alarm.core.Alarm"></event-type>
</esper-configuration>

